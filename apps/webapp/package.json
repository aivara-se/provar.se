{
	"name": "webapp",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && ./scripts/postbuild.sh",
		"build:docker": "docker build -t provar-webapp:latest .",
		"preview": "vite preview",
		"preview:docker": "docker run -it --rm --env-file ./.env -p 5173:5173 provar-webapp:latest",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . || echo 'Linting failed'",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@auth/core": "^0.27.0",
		"@auth/mongodb-adapter": "^2.4.0",
		"@auth/sveltekit": "^0.13.0",
		"@google-cloud/storage": "^7.7.0",
		"@provar/provar-js": "^1.3.1",
		"chart.js": "^4.4.1",
		"chartjs-adapter-date-fns": "^3.0.0",
		"daisyui": "^4.7.2",
		"date-fns": "^3.3.1",
		"js-sha256": "^0.11.0",
		"lodash": "^4.17.21",
		"lucide-svelte": "^0.338.0",
		"mongodb": "^6.3.0",
		"nodemailer": "^6.9.9",
		"overlayscrollbars": "^2.5.0",
		"overlayscrollbars-svelte": "^0.5.3",
		"qs": "^6.11.2",
		"sveltekit-superforms": "^2.2.0",
		"tslib": "^2.4.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "8.56.0",
		"@types/lodash": "^4.14.202",
		"@types/nodemailer": "^6.4.14",
		"@types/qs": "^6.9.11",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.4",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.0"
	}
}

{
	"name": "webapp",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && ./scripts/postbuild.sh",
		"build:docker": "docker build -t provar-webapp:latest .",
		"preview": "vite preview",
		"preview:docker": "docker run -it --rm --env-file ./.env -p 5173:5173 provar-webapp:latest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin=prettier-plugin-svelte . --check . & eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte . --write . && eslint --fix .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@auth/core": "^0.25.0",
		"@auth/mongodb-adapter": "^2.1.0",
		"@auth/sveltekit": "^0.11.0",
		"@google-cloud/storage": "^7.7.0",
		"@provar/provar-js": "^1.3.1",
		"chart.js": "^4.4.1",
		"chartjs-adapter-date-fns": "^3.0.0",
		"date-fns": "^3.3.1",
		"lodash": "^4.17.21",
		"mongodb": "^6.3.0",
		"nodemailer": "^6.9.9",
		"qs": "^6.11.2",
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.4.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/lodash": "^4.14.202",
		"@types/nodemailer": "^6.4.14",
		"@types/qs": "^6.9.11",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.16.0",
		"@vitest/coverage-v8": "^1.1.3",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"flowbite-svelte-blocks": "^0.5.1",
		"flowbite-svelte-icons": "^1.0.3",
		"postcss": "^8.4.33",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.2.2",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.3",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.0"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}

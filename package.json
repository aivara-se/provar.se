{
  "name": "monorepo",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn workspaces foreach -Wpi --from '{webapi,webapp,website,collect}' run dev",
    "setup-local-env": "bash .scripts/setup-local-env.sh",
    "start-local-env": "bash .scripts/start-local-env.sh"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "infra/*"
  ],
  "devDependencies": {
    "@yarnpkg/types": "^4.0.0"
  },
  "engines": {
    "node": ">=23.0.0"
  },
  "packageManager": "yarn@4.6.0"
}
